        {% set items = [] %}
        {% for inputParams in input.value %}
            {% set explodedParams = inputParams | split(',') -%}

            {% for param in explodedParams -%}
                {% set paramName = param | split('$') -%}
                {% set item = '$' ~ paramName[1] -%}
                {% set items = items | merge([item]) %}
            {% endfor -%}
        {% endfor -%}

        ${{ zedDomainEntity | lcfirst }}CollectionDeleteCriteriaTransfer = $this->getFactory()
            ->createGlueRequest{{ zedDomainEntity }}Mapper()
            ->mapIdentifierTo{{ zedDomainEntity }}CollectionDeleteCriteriaTransfer({{ items | join(', ') }});

        ${{ zedDomainEntity | lcfirst }}CollectionResponseTransfer = $this->getFactory()
            ->get{{ dataModule }}Facade()
            ->delete{{ zedDomainEntity }}Collection(${{ zedDomainEntity | lcfirst }}CollectionDeleteCriteriaTransfer);

        return $this->getFactory()
            ->createGlueResponse{{ zedDomainEntity }}Mapper()
            ->map{{ zedDomainEntity }}CollectionResponseTransferToSingleResourceGlueResponseTransfer(${{ zedDomainEntity | lcfirst }}CollectionResponseTransfer);
